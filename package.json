{
  "name": "ipfs-node",
  "version": "0.0.6",
  "description": "This repository allows you to create your own ipfs node when using helia.",
  "author": "https://github.com/0xdino",
  "private": false,
  "keywords": [
    "ipfs",
    "helia",
    "ipfs-node"
  ],
  "repository": "https://github.com/0xdino/ipfs-node.git",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "test": "tsx ./test/index.ts",
    "lint": "pnpm prettier:write && pnpm prettier:check && eslint \"./src/*.ts\" --fix",
    "prettier:check": "prettier --check ./**/*.{ts,js,json,*rc}",
    "prettier:write": "prettier --write ./**/*.{ts,js,json,*rc}",
    "precommit": "pnpm lint",
    "prepare": "husky install",
    "prepush": "pnpm lint",
    "build": "rimraf ./dist && tsc && pnpm babel",
    "babel": "babel dist --out-dir dist --extensions .js"
  },
  "exports": {
    ".": [
      "./dist/index.js"
    ]
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "kubo-rpc-client": "^3.0.2",
    "multiformats": "^13.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "husky": "^8.0.3",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
